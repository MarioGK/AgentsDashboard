{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Worker": {
    "UseDocker": true,
    "DefaultImage": "ai-harness:latest",
    "HarnessImages": {
      "codex": "ai-harness:latest",
      "opencode": "ai-harness:latest",
      "claude code": "ai-harness:latest",
      "claude-code": "ai-harness:latest",
      "zai": "ai-harness:latest"
    }
  },
  "Terminal": {
    "IdleTimeoutMinutes": 30,
    "ResumeGraceMinutes": 10,
    "MaxConcurrentSessionsPerWorker": 1,
    "MaxChunkBytes": 8192,
    "DefaultImage": "ai-harness:latest"
  }
}
