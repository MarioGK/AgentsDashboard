{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Orchestrator": {
    "SqliteConnectionString": "Data Source=/data/orchestrator.db",
    "WorkerGrpcAddress": "http://localhost:5201",
    "WorkerContainerImage": "agentsdashboard-worker-gateway:latest",
    "WorkerContainerName": "worker-gateway",
    "WorkerDockerNetwork": "agentsdashboard",
    "SchedulerIntervalSeconds": 10,
    "MaxGlobalConcurrentRuns": 50
  }
}
