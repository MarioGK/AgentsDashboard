{
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
    }
  },
  "Orchestrator": {
    "LiteDbPath": "data/litedb/orchestrator.db",
    "ArtifactsRootPath": "data/artifacts",
    "TaskRuntimes": {
      "MaxTaskRuntimes": 100,
      "ParallelSlotsPerTaskRuntime": 1,
      "IdleTimeoutMinutes": 5,
      "StartupTimeoutSeconds": 60,
      "ContainerImage": "ghcr.io/mariogk/ai-harness:latest",
      "ContainerNamePrefix": "task-runtime",
      "DockerNetwork": "agentsdashboard",
      "ConnectivityMode": "AutoDetect",
      "EnablePressureScaling": true,
      "CpuScaleOutThresholdPercent": 85,
      "MemoryScaleOutThresholdPercent": 85,
      "PressureSampleWindowSeconds": 30,
      "EnableHostSshPassthrough": true,
      "HostSshDirectory": "",
      "HostSshAgentSocketPath": "",
      "GitSshCommandMode": "no"
    },
    "SchedulerIntervalSeconds": 10,
    "MaxGlobalConcurrentRuns": 50
  }
}
